  "eslintConfig": {
    "extends": [
      "react-app",
      "airbnb",
      "airbnb/hooks"
    ],
    "rules": {
      "react/jsx-props-no-spreading": 0,
      "react/prop-types": 0,
      "react/jsx-fragments": 0,
      "no-nested-ternary": 0,
      "import/no-unresolved": 0,
      "import/no-named-as-default": 0,
      "no-unused-expressions": 0,
      "comma-dangle": 0,
      "indent": [
        2,
        2,
        {
